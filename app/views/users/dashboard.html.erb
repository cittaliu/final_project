
<%= render partial: '/partials/header' %>
<%= render partial: '/partials/side' %>

<% @auth.each do |key, value| %>
  <h1><%= key %> </h1>
  <p><%= value %> </p>
<%end%>


<div id="content" class="app-content" role="main">
  <div class="app-content-body ">

    <div class="bg-light lter b-b wrapper-md">
      <h1 class="m-n font-thin h3">I'm dashboard.html.erb</h1>
    </div>
    <div class="wrapper-md">
      <h3 class="font-thin m-t-xs">My Events this Month</h3>
    </div>

    <div>
    <%  @events.each do |item| %>
    <div class="panel">
      <div class="wrapper-lg">
      <%  @summary = item['summary'] %>
      <%  @start = item['start'].to_s[26..-2] %>
      <%  @end_date = item['end'].to_s[26..-2] %>
      <%  @location = item['location'].to_s[42..-3] %>
      <%= @summary %>
      <br>
      <%= @start %>
      <br>
      <%= @end_date %>
      <br>
      <%= @location %>
    </div>
  </div>
    <%  end %>
    </div>

  </div>
</div>
<%= render partial: '/partials/footer' %>
